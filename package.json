    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@vitejs/plugin-vue": "^2.3.3",
    "lodash-es": "^4.17.21",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitepress": "1.0.0-alpha.2",
    "vue-tsc": "^0.34.17"
